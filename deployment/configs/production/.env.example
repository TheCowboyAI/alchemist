# Production Environment Variables for Alchemist
# Copy this file to .env and fill in the values

# Agent Configuration
AGENT_ID=alchemist-prod-001
HOSTNAME=alchemist-prod-001

# AI Provider Keys
ANTHROPIC_API_KEY=your-anthropic-api-key
OPENAI_API_KEY=your-openai-api-key
AI_PROVIDER=anthropic

# NATS Configuration
NATS_URL=nats://nats:4222

# Database Configuration
DATABASE_URL=postgresql://alchemist:password@postgres:5432/alchemist?sslmode=require

# Redis Configuration
REDIS_URL=redis://redis:6379/1
REDIS_PASSWORD=your-redis-password

# S3 Storage Configuration
S3_ENDPOINT=https://s3.amazonaws.com
S3_BUCKET=alchemist-prod
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key

# Security
JWT_SECRET=your-very-long-random-jwt-secret
ALCHEMIST_JWT_SECRET=your-very-long-random-jwt-secret
ALCHEMIST_API_KEY=your-api-key

# Monitoring
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4317

# Logging
LOG_LEVEL=warn
APP_VERSION=1.0.0

# Feature Flags
ENABLE_EXPERIMENTAL=false
ENABLE_BETA=true